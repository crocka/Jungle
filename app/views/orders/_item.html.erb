<tr>
  <td>
    <%= image_tag product.image.tiny %>
  </td>
  <td>
    <h4><%= product.name %></h4>
    <br>
    <p><%= product.description %></p>
  </td>
  <td>
    <%= quantity %>
  </td>

  <td>
   <%= humanized_money_with_symbol product.price_cents / 100 %>
  </td>

  <td>
    <%= humanized_money_with_symbol product.price_cents * quantity / 100 %>
  </td>


</tr>
